<Interaction Type="Tabs" ContentFolder="proj_content" Title="Rules" Numbering="0" animation="bounce" contentfont="Articulate" tabwidth="192" jpegquality="80" autoplay="false" autoplayduration="5" headingfont="Articulate" navfont="Articulate" soundeffects="true" volumebutton="true" audioquality="low" audiocustombitrate="br112Kbps" audiocustomfrequency="fq12KHz" audiocustomchannel="ccMono" navbuttons="true" forceorder="false" detectflashver="True" version="1.2.731.151" borderstyle="round_inset">
  <steps>
    <Step id="0c325a8e-69d1-4148-8fa3-4b4ffd3dced4" name="Remote Staff Rules and Regulation" title="" number="" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Remote Staff Rules and Regulation&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;To ensure the high quality of the services we provide to our clients you would need to follow the company's rules and regulations. &lt;/font&gt;&lt;/p&gt;" intro="true">
      <image width="265" height="189" zoomheight="501" playbar="false" autoplay="false" zoomwidth="700" zoommode="Embedded" allowzoom="true" align="bottom" duration="0" name="image0.jpg" zoomimage="imageZoom0.jpg" />
    </Step>
    <Step id="8ffe74c5-d6f7-4455-b1ba-b47f8df638c7" name="Skype Response" title="" number="" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Skype Response&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;You will need to answer to client's Skype message within 3 minutes. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;If you happen to be on the phone while the message is sent, send a quick message that says &lt;i&gt;&quot;on the phone&quot;&lt;/i&gt; so the client knows what you are up to. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;The client will report you to Remote Staff if you fail to respond to them within this time frame maximum of 3 times a day. &lt;/font&gt;&lt;/p&gt;">
      <image width="190" height="190" zoomheight="300" playbar="false" autoplay="false" zoomwidth="300" zoommode="Embedded" allowzoom="true" align="bottom" duration="0" name="image1.jpg" zoomimage="imageZoom1.jpg" />
    </Step>
    <Step id="18560d9d-7dca-43c0-9473-6c34348c04c3" name="Screen Share" title="" number="" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Screen Share&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;When a client requires you to share your screen with him/her you should abide. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;IF this can't be done, you should provide a valid reason. &lt;/font&gt;&lt;/p&gt;">
      <image width="167" height="189" zoomheight="340" playbar="false" autoplay="false" zoomwidth="300" zoommode="Embedded" allowzoom="true" align="bottom" duration="0" name="image2.jpg" zoomimage="imageZoom2.jpg" />
    </Step>
    <Step id="3bae6bba-bbe6-4762-9960-f70d31665dcc" name="Lunch Breaks" title="" number="" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;Lunch Breaks&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;All remote staff should take their lunch breaks at 12:30 PM Manila time unless requested by the client. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;A note that says LUNCH on your Skype should be visible. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;Lunch break should only last for an hour.  &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;Don't forget to log out of the RemoteStaff system when on a lunch break. &lt;/font&gt;&lt;/p&gt;">
      <image width="197" height="189" zoomheight="520" playbar="false" autoplay="false" zoomwidth="539" zoommode="Embedded" allowzoom="true" align="bottom" duration="0" name="image3.jpg" zoomimage="imageZoom3.jpg" />
    </Step>
    <Step id="f51205ea-26e6-4197-8c7b-6194543c96d1" name="Overtime" title="" number="" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Overtime&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;When overtime is requested from you by the client, you would need to inform RemoteStaff HR manager about it, or you wont be paid extra. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;&lt;b&gt;&lt;u&gt;Over time&lt;/u&gt;&lt;/b&gt; is defined as working over the 9-hours worksheet you are subcontracted for or working on Saturdays, Sundays and Holidays. &lt;/font&gt;&lt;/p&gt;">
      <image width="188" height="189" zoomheight="232" playbar="false" autoplay="false" zoomwidth="230" zoommode="Embedded" allowzoom="true" align="bottom" duration="0" name="image4.jpg" zoomimage="imageZoom4.jpg" />
    </Step>
    <Step id="a2d85602-4ab6-48f0-a0cb-0a306526948f" name="Holiday" title="" number="" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Holiday&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;b&gt;&lt;font face='Articulate' size='16' color=''&gt;We follow National Australian Holiday. Please refer to the RemoteStaff Manual regarding this. &lt;/font&gt;&lt;/b&gt;&lt;/p&gt;">
      <image width="157" height="189" zoomheight="360" playbar="false" autoplay="false" zoomwidth="300" zoommode="Embedded" allowzoom="true" align="bottom" duration="0" name="image5.jpg" zoomimage="imageZoom5.jpg" />
    </Step>
    <Step id="48746e11-614d-4e54-bc61-5f540c28272e" name="Time Sheets" title="" number="" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Time Sheets&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;RemoteStaff System will pop up random messages on your screen during working hours. You would need to respond to this checking system as this will serve as your time sheet. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;The time you log in and Log out will be recorded on the system.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;These factors determine what will be on your time sheet. &lt;/font&gt;&lt;/p&gt;">
      <image width="169" height="189" zoomheight="475" playbar="false" autoplay="false" zoomwidth="423" zoommode="Embedded" allowzoom="true" align="bottom" duration="0" name="image6.jpg" zoomimage="imageZoom6.jpg" />
    </Step>
    <Step id="0b8f8849-b08b-48f1-8029-97de8e320598" name="END" title="" number="" html="">
    </Step>
  </steps>
