<Interaction Type="Process" ContentFolder="proj_content" Title="Google Documents" Numbering="0" animation="bounce" contentfont="Articulate" jpegquality="80" autoplay="false" autoplayduration="5" headingfont="Articulate" navfont="Articulate" soundeffects="true" volumebutton="true" audioquality="low" audiocustombitrate="br112Kbps" audiocustomfrequency="fq12KHz" audiocustomchannel="ccMono" navbuttons="true" forceorder="false" detectflashver="True" version="1.2.731.151" borderstyle="round_inset">
  <steps>
    <Step id="84356f6e-e29a-4b9d-82cf-e94fd4c03663" name="Google Documents" title="" number="0" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color='#000000'&gt;&lt;b&gt;Google Documents&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color='#000000'&gt;This page will give you a step by step instruction on how to go about using Google Documents or Google Docs.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color='#000000'&gt;Click on the images to enlarge it. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color='#000000'&gt;Please click the numbers below to go through the steps. &lt;/font&gt;&lt;/p&gt;" intro="true">
      <image width="248" height="138" zoomheight="319" playbar="false" autoplay="false" zoomwidth="570" zoommode="Embedded" allowzoom="true" align="topright" duration="0" name="image0.jpg" zoomimage="imageZoom0.jpg" />
    </Step>
    <Step id="d76213d2-11bc-483c-a1ab-8b956834f679" name="Step 1: Go to Google Page" title="" number="1" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color='#000000'&gt;&lt;b&gt;Go to Google Page&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color='#000000'&gt;Go to www.google.com and click on Documents.&lt;/font&gt;&lt;/p&gt;">
      <image width="248" height="155" zoomheight="437" playbar="false" autoplay="false" zoomwidth="700" zoommode="Embedded" allowzoom="true" align="topright" duration="0" name="image1.jpg" zoomimage="imageZoom1.jpg" />
    </Step>
    <Step id="ebdb5962-c060-4255-a303-862edb189120" name="Step 2: Log In to your Google Account" title="" number="2" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color='#000000'&gt;&lt;b&gt;Log In to your Google Account&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color='#000000'&gt;If you are not log in to your Google account, Google will ask you to do so. Logging in will automatically bring you to the Google Docs Page.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color='#000000'&gt;If you don't have a Google Email account, please press GET STARTED and sign up.&lt;br&gt;&lt;/br&gt; &lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color='#000000'&gt;&lt;/font&gt;&lt;/p&gt;">
      <image width="248" height="155" zoomheight="437" playbar="false" autoplay="false" zoomwidth="700" zoommode="Embedded" allowzoom="true" align="topright" duration="0" name="image2.jpg" zoomimage="imageZoom2.jpg" />
    </Step>
    <Step id="96d84ee0-d68c-415e-ac1f-92205b728cb2" name="Step 3: Google Docs Page" title="" number="3" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Google Docs Page&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;This is the page where you can see all your Google documents. Clicking on one will bring you to an Excel spreadsheet-LIKE window.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;THAT is your Google document.&lt;/font&gt;&lt;/p&gt;">
      <image width="248" height="155" zoomheight="437" playbar="false" autoplay="false" zoomwidth="700" zoommode="Embedded" allowzoom="true" align="topright" duration="0" name="image3.jpg" zoomimage="imageZoom3.jpg" />
    </Step>
    <Step id="f9809328-f933-47af-82ae-b595ca41580f" name="Step 4: Google Docs" title="" number="4" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Google Docs&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;This is an example of one Google document sheet. &lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;In this case, it's a Workflow Sheet. You can see task assigned per day and the status of work being done.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;If you scroll down, you will see all the participant in this sheet. (Refer to Step 6)&lt;/font&gt;&lt;/p&gt;">
      <image width="248" height="120" zoomheight="340" playbar="false" autoplay="false" zoomwidth="700" zoommode="Embedded" allowzoom="true" align="topright" duration="0" name="image4.jpg" zoomimage="imageZoom4.jpg" />
    </Step>
    <Step id="7c0fc642-4540-495c-95a3-483ab84e8f6f" name="Step 5: Editing Google Docs Spreadsheet" title="" number="5" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;Editing Google Docs Spreadsheet&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;You could type and edit anything you want on the sheet the way you type it on an Excel Spreadsheet.&lt;br&gt;&lt;/br&gt;&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;Functions are pretty much the same.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;">
      <image width="248" height="155" zoomheight="437" playbar="false" autoplay="false" zoomwidth="700" zoommode="Embedded" allowzoom="true" align="topright" duration="0" name="image5.jpg" zoomimage="imageZoom5.jpg" />
    </Step>
    <Step id="eb44060a-0fdd-41ff-bfa5-7fa8d479f2ac" name="Step 6 : Adding Sheet, Collaborators and Saving" title="" number="6" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Adding Sheet, Collaborators and Saving&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;To add a new sheet, click &quot;Add sheet&quot; at the bottom. This is highlighted for you.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;The collaborators, the people who could view and edit his document, can be seen below. To the right of the &quot;Add Sheet&quot; function. These people can see your sheet and you can check our their sheets as well.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;Before closing the application, don't forget to click &quot;Save and Close&quot; button on the upper right hand side corner of the page. This will save changes you have made. &lt;/font&gt;&lt;/p&gt;">
      <image width="248" height="136" zoomheight="385" playbar="false" autoplay="false" zoomwidth="700" zoommode="Embedded" allowzoom="true" align="topright" duration="0" name="image6.jpg" zoomimage="imageZoom6.jpg" />
    </Step>
    <Step id="c88eea96-c6ff-4c4e-86cf-eca8699fdd8f" name="Step 7: Viewing and Chatting with Collaborators" title="" number="7" html="&lt;p align='center'&gt;&lt;font face='Articulate' size='24' color=''&gt;&lt;b&gt;Viewing and Chatting with Collaborators&lt;br&gt;&lt;/br&gt;&lt;/b&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;You could view who's currently on the Google docs by looking at the right hand corner of the document where it says &quot;Viewing Now&quot;. The names you see on that box are currently updating their documents.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;To chat with online collaborators, type in your message on the small box below the names and press &quot;enter&quot;.&lt;br&gt;&lt;/br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;In this case, there's no collaborators online. Notice that i have typed in a message that didn't go anywhere. &lt;/font&gt;&lt;/p&gt;">
      <image width="248" height="120" zoomheight="340" playbar="false" autoplay="false" zoomwidth="700" zoommode="Embedded" allowzoom="true" align="topright" duration="0" name="image7.jpg" zoomimage="imageZoom7.jpg" />
    </Step>
    <Step id="a1a1ffaa-8170-4f9b-a6b2-b72dc3597e79" name="END" title="" number="8" html="&lt;p align='left'&gt;&lt;font face='Articulate' size='16' color=''&gt;That's the end of this Google docs tutorial. &lt;/font&gt;&lt;/p&gt;">
    </Step>
  </steps>
